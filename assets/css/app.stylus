body
  color: #24292e;
  font-feature-settings: "palt";
  -webkit-font-smoothing: antialiased;
  font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,YuGothic,Yu Gothic,sans-serif;

a
  text-decoration none

@media (min-width: 768px)
  .container
    width 750px

@media (min-width: 992px)
  .container
    width 970px

@media (min-width: 1220px)
  .container
    width 1170px

.container
  margin-right auto
  margin-left auto
  padding-left 15px
  padding-right 15px
